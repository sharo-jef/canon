struct TemplateTest {
    name: string = "Test"
    count: int = 5
    
    // Simple interpolation
    simple: string = `Hello $name!`
    
    // Complex expression interpolation
    complex: string = `Count is ${count + 1}, doubled is ${count * 2}`
    
    // Member access interpolation  
    memberAccess: string = `Version: ${this.name}`
    
    // Nested expressions
    nested: string = `Result: ${(count + 1) * 2}`
}
