// Test else if pattern
struct ElseIfTest {
    value: int = 10
    
    // Test nested if for else if pattern
    category: string = if (value < 0) {
        "negative"
    } else if (value == 0) {
        "zero"  
    } else if (value < 10) {
        "small positive"
    } else {
        "large positive"
    }
}
